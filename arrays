#include <iostream>
#include <climits>
using namespace std;
int main() {
    int smaller=INT_MAX;
    int maximum=INT_MIN;
    int array[5]={54,34,45,67,22};
    int size=5;
    for(int i=0;i<size;i++)
    {
    if(array[i]>maximum)
    {
        maximum=array[i];
    }
        
    }
    cout << "the smaller value is"<< maximum<<endl;
    return 0;
}
