#include <iostream>
using namespace std;
int main() {
  int n, i;
  i=1;
  cin>>n; 
  while(i<=n)
  {
      int j =1;
      while(j<=n)      //54321 and neeche bi 54321

      {
          cout<<n-j+1;
          j++;
      }
      cout<<endl;
      i++;
  }
    return 0;
}





#include <iostream>
using namespace std;
int main() {
    int i,n;
    cin>>n;
    i=1;
    while(i<=n){
        int space = n-i;
        while(space)      //First iteration (when i = 1):space = 5 - 1 = 4
                        The inner while (space) loop will run 4 times, printing 4 spaces (" ").
After each loop, space-- reduces space from 4 to 0, and then the loop exits.
        {
        cout<<" ";
        space--;
        }
    int j = 1;
    while(j<=i)
    {
        cout<<"*";
        j++;
    }
     i++;
        cout<<endl;
    }
    
    return 0;
}






#include <iostream>
#include <math.h>
using namespace std;
int main() {
    int i,n;
    cin>>n;
    i=0;
    int ans =0;
    while(n)      ///decimal to binary
    {
        int bit=n&1;
        ans=(bit*pow(10,i))+ans;
        n = n>>1;
        i++;
    }
    cout<<"answer is:"<<ans<<endl;

    return 0;
}


#include <iostream>
#include <math.h>
using namespace std;
int main()
{
    int i,n;
    cin>>n;
    int ans;
    int digit;
    ans=0;
    i=0;
    while(n)
    {
        digit=n%10;   ///esme fir se n=n%10 nhi karenge kyuki abhi esme kiye toh n ka value 1 ya 0ho jayega and neeche we cant do n/10 so
        if(digit==1)
        {
         ans=ans+pow(2,i) ;  
        }
        n=n/10;
        i++;
    }
    cout<<ans<<endl;
}
